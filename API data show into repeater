Back-end
import {fetch} from 'wix-fetch'
export async  function api(){
    let data= await fetch ('https://jsonplaceholder.typicode.com/posts')
    return  data.json()
}
Front-end
import {api} from 'backend/data.jsw'
$w.onReady( async function () {
 let apidata= await api()
 console.log(apidata)
 let currentdata=await apidata.map(result=>{
 return ({'_id':result.id.toString(),'body':result.body})
 })
 $w('#repeater').data=currentdata
});
export function repeater_itemReady($item,itemData) {
$item('#fd').text=itemData.body
}
